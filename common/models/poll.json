{
  "name": "Poll",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "questions": {
      "type": [
        "object"
      ]
    },
    "description": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "instructions": {
      "type": "string"
    },
    "sent": {
      "type": "boolean"
    },
    "expiration_limit": {
      "type": "date"
    },
    "iteration":{
      "type": "number",
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "experts": {
      "type": "hasMany",
      "model": "Expert",
      "foreignKey": "pollId",
      "through": "Result"
    },
    "investigation": {
      "type": "belongsTo",
      "model": "Investigation",
      "foreignKey": "investigationId"
    },
    "results": {
      "type": "hasMany",
      "model": "Result",
      "foreignKey": ""
    }
  },
  "acls": [

  ],
  "methods": {}
}
