{
  "name": "Poll",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "questions": {
      "type": [
        "string"
      ]
    },
    "description": {
      "type": "string"
    },
    "title": {
      "type": "string"

    },
    "instructions": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "sent": {
      "type": "boolean"
    },
    "expiration_limit":{
      "type":"date"
    }
  },
  "validations": [],
  "relations": {
    "experts": {
      "type": "hasMany",
      "model": "Expert",
      "foreignKey": "pollId",
      "through": "Result"
    },
    "investigations": {
      "type": "belongsTo",
      "model": "Investigation",
      "foreignKey": "investigationId"
    },
    "results": {
      "type": "hasMany",
      "model": "Result",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
